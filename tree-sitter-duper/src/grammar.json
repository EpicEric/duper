        },
        {
          "type": "SYMBOL",
          "name": "boolean"
        },
        {
          "type": "SYMBOL",
          "name": "null"
        }
      ]
    },
    "object": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "{"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "object_entry"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "object_entry"
                      }
                    ]
                  }
                },
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "STRING",
                      "value": ","
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "}"
        }
      ]
    },
    "array": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "duper_value"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "duper_value"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "tuple": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "duper_value"
                },
                {
                  "type": "REPEAT",
                  "content": {
                    "type": "SEQ",
                    "members": [
                      {
                        "type": "STRING",
                        "value": ","
                      },
                      {
                        "type": "SYMBOL",
                        "name": "duper_value"
                      }
                    ]
                  }
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "STRING",
              "value": ","
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "string": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "raw_string"
        }
      ]
    },
    "bytes": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "quoted_bytes"
        },
        {
          "type": "SYMBOL",
          "name": "raw_bytes"
        },
        {
          "type": "SYMBOL",
          "name": "base64_bytes"
        }
      ]
    },
    "temporal": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "'"
        },
        {
          "type": "SYMBOL",
          "name": "temporal_content"
        },
        {
          "type": "STRING",
          "value": "'"
        }
      ]
    },
    "integer": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "decimal_integer"
        },
        {
          "type": "SYMBOL",
          "name": "hex_integer"
        },
        {
          "type": "SYMBOL",
          "name": "octal_integer"
        },
        {
          "type": "SYMBOL",
          "name": "binary_integer"
        }
      ]
    },
    "float": {
      "type": "PATTERN",
      "value": "[+-]?([0-9]|[1-9](_?[0-9])+)(\\.[0-9](_?[0-9])*([eE][+-]?([0-9]|[1-9](_?[0-9])+))?|[eE][+-]?([0-9]|[1-9](_?[0-9])+))"
    },
    "boolean": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        }
      ]
    },
    "null": {
      "type": "STRING",
      "value": "null"
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-Z]([_-]?[a-zA-Z0-9])*"
    },
    "object_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "object_key"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "duper_value"
        }
      ]
    },
    "object_key": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "plain_key"
        },
        {
          "type": "SYMBOL",
          "name": "quoted_string"
        },
        {
          "type": "SYMBOL",
          "name": "raw_string"
        }
      ]
    },
    "plain_key": {
      "type": "PATTERN",
      "value": "(_[a-zA-Z0-9]|[a-zA-Z])([_-]?[a-zA-Z0-9])*"
    },
    "quoted_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "SYMBOL",
          "name": "quoted_content"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "raw_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "r"
        },
        {
          "type": "SYMBOL",
          "name": "raw_start"
        },
        {
          "type": "SYMBOL",
          "name": "raw_content"
        },
        {
          "type": "SYMBOL",
          "name": "raw_end"
        }
      ]
    },
    "quoted_bytes": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "b\""
        },
        {
          "type": "SYMBOL",
          "name": "quoted_content"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "raw_bytes": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "br"
        },
        {
          "type": "SYMBOL",
          "name": "raw_start"
        },
        {
          "type": "SYMBOL",
          "name": "raw_content"
        },
        {
          "type": "SYMBOL",
          "name": "raw_end"
        }
      ]
    },
    "base64_bytes": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "b64\""
        },
        {
          "type": "SYMBOL",
          "name": "base64_content"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "quoted_content": {
      "type": "PATTERN",
      "value": "(\\\\\"|[^\"])*"
    },
    "base64_content": {
      "type": "PATTERN",
      "value": "[a-zA-Z0-9+/ \\t\\r\\n]*[= \\t\\r\\n]*"
    },
    "temporal_content": {
      "type": "PATTERN",
      "value": "[ \\t\\r\\n]*[^' \\t\\r\\n][^']+[^' \\t\\r\\n][ \\t\\r\\n]*"
    },
    "decimal_integer": {
      "type": "PATTERN",
      "value": "[+-]?([0-9]|[1-9](_?[0-9])+)"
    },
    "hex_integer": {
      "type": "PATTERN",
      "value": "0x[0-9a-fA-F](_?[0-9a-fA-F])*"
    },
    "octal_integer": {
      "type": "PATTERN",
      "value": "0o[0-7](_?[0-7])*"
    },
    "binary_integer": {
      "type": "PATTERN",
      "value": "0b[01](_?[01])*"
    },
    "line_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "//"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    },
    "block_comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "/*"
          },
          {
            "type": "PATTERN",
            "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
          },
          {
            "type": "STRING",
            "value": "/"
          }
        ]
      }
    }
  },
  "extras": [
