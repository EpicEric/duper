{
  "name": "@duper-js/wasm",
  "version": "0.2.1",
  "description": "The format that's super!",
  "keywords": [
    "duper",
    "serialization",
    "wasm"
  ],
  "homepage": "https://duper.dev.br",
  "bugs": {
    "url": "https://github.com/EpicEric/duper/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EpicEric/duper.git"
  },
  "license": "MIT",
  "author": "Eric Rodrigues Pires <eric@eric.dev.br>",
  "type": "module",
  "main": "./pkg/duper_js_wasm.js",
  "types": "./pkg/duper_js_wasm.d.ts",
  "exports": {
    ".": {
      "import": "./pkg/duper_js_wasm.js",
      "types": "./pkg/duper_js_wasm.d.ts"
    },
    "./wasm": {
      "import": "./pkg/duper_js_wasm_bg.wasm"
    }
  },
  "files": [
    "pkg/*",
    "README.md"
  ],
  "scripts": {
    "build": "cargo build --target wasm32-unknown-unknown --release && wasm-bindgen ../target/wasm32-unknown-unknown/release/duper_js_wasm.wasm --out-dir ./pkg --target bundler",
    "optimize": "wasm-opt -Os ./pkg/duper_js_wasm_bg.wasm -o ./pkg/duper_js_wasm_bg.wasm",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "prepublishOnly": "npm run build && npm run optimize"
  },
  "devDependencies": {
    "vite": "^7.1.12",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0",
    "vitest": "^4.0.6"
  },
  "dependencies": {
    "temporal-polyfill": "^0.3.0"
  }
}
