{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "duper",
  "rules": {
    "duper_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identified_value"
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        }
      ]
    },
    "identified_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "_value"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "_value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "object"
        },
        {
          "type": "SYMBOL",
          "name": "array"
        },
        {
          "type": "SYMBOL",
          "name": "tuple"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "bytes"
        },
        {
          "type": "SYMBOL",
          "name": "temporal"
        },
        {
          "type": "SYMBOL",
          "name": "float"
        },
        {
          "type": "SYMBOL",
