{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--disable-extensions",
        "--extensionDevelopmentPath=${workspaceFolder}/duper-vs-code"
      ],
      "outFiles": [
        "${workspaceFolder}/duper-vs-code/dist/**/*"
      ],
      "preLaunchTask": "npm: prelaunch - duper-vs-code"
    }
  ]
}